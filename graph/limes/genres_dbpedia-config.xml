<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
  <PREFIX>
    <NAMESPACE>http://semweb.mmlab.be/ns/rml#</NAMESPACE>
    <LABEL>rml</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://semweb.mmlab.be/ns/ql#</NAMESPACE>
    <LABEL>ql</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/ns/r2rml#</NAMESPACE>
    <LABEL>rr</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
    <LABEL>rdf</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
    <LABEL>rdfs</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/2001/XMLSchema#</NAMESPACE>
    <LABEL>xsd</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
    <LABEL>foaf</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://linkedgeodata.org/page/ontology/</NAMESPACE>
    <LABEL>lgdo</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://purl.org/ontology/mo/</NAMESPACE>
    <LABEL>mo</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://purl.org/dc/elements/1.1/</NAMESPACE>
    <LABEL>dc</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
    <LABEL>dcterms</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
    <LABEL>dbo</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
    <LABEL>dbp</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
    <LABEL>skos</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://open.vocab.org/terms/</NAMESPACE>
    <LABEL>ov</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://purl.org/vocab/bio/0.1/</NAMESPACE>
    <LABEL>bio</LABEL>
  </PREFIX>
  <PREFIX>
    <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
    <LABEL>owl</LABEL>
  </PREFIX>
  <SOURCE>
    <ID>musicbrainz</ID>
    <ENDPOINT>http://137.120.219.156:7200/repositories/musicbrainz</ENDPOINT>
    <VAR>?x</VAR>
    <PAGESIZE>-1</PAGESIZE>
    <RESTRICTION>?x a mo:Genre</RESTRICTION>
    <PROPERTY>rdfs:label AS lowercase RENAME name</PROPERTY>
    <TYPE>sparql</TYPE>
  </SOURCE>
  <TARGET>
    <ID>dbpedia</ID>
    <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
    <VAR>?y</VAR>
    <PAGESIZE>5000</PAGESIZE>
    <RESTRICTION>?y a dbo:Genre</RESTRICTION>
    <PROPERTY>rdfs:label AS nolang->lowercase RENAME name</PROPERTY>
    <TYPE>sparql</TYPE>
  </TARGET>
  <METRIC>levenshtein(y.name,x.name)</METRIC>
  <ACCEPTANCE>
    <THRESHOLD>0.98</THRESHOLD>
    <FILE>accepted3.nt</FILE>
    <RELATION>owl:sameAs</RELATION>
  </ACCEPTANCE>
  <REVIEW>
    <THRESHOLD>0.5</THRESHOLD>
    <FILE>reviewme3.nt</FILE>
    <RELATION>owl:sameAs</RELATION>
  </REVIEW>
  <OUTPUT>nt</OUTPUT>
</LIMES>